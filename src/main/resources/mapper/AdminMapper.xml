<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dark.graduations.mapper.AdminMapper">
    <select id="querryByAdminAcount" resultType="String">
        select Admin_Pwd from admin where Admin_Acount = #{username}
    </select>

    <select id="querryAllStudent" resultType="com.dark.graduations.pojo.Student">
        select * from student
    </select>
</mapper>